{
  "extends": [
    "development"
  ],
  "hints": {
    "meta-viewport": "off",
    "meta-charset-utf-8": "off",
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off",
        "frame-title": "off",
        "document-title": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "axe/forms": "off",
    "no-inline-styles": "off",
    "button-type": "off",
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 119",
    "not firefox <= 120",
    "not ios_saf <= 17.1",
    "not safari <= 17.1"
  ]
}